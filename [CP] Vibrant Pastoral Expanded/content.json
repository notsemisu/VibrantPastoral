{
	"Format": "2.2.0",
	"ConfigSchema": {
		"Buildings": {
			"AllowValues": "extras, lighthouse, morris",
			"AllowMultiple": true,
			"AllowBlank": true,
			"Default": "extras, lighthouse, morris",
			"Description": "Enable changes to specified buildings.",
            "Section": "Buildings"},
		"Shrubbery": {
			"AllowValues": "false, Simple Foliage, Vanilla",
			"AllowBlank": true,
			"Default": "",
			"Description": "Manually set bushes outside Olivia's/Victor's house.",
            "Section": "Buildings"}
	},
	"DynamicTokens": [
		{
			"Name": "Seasonal",
			"Value": "_{{Season}}"
		},
		{
			"Name": "Seasonal",
			"Value": "",
			"When": { "Season": "Spring" }
		},
		{
			"Name": "SeasonOverride",
			"Value": "{{Season}}"
		},
		{
			"Name": "SeasonOverride",
			"Value": "Spring",
			"When": { "Query: NOT {{IsOutdoors}} OR {{LocationName |contains=Desert}}": true}
		},
		{
			"Name": "SeasonOverride",
			"Value": "Summer",
			"When": { "LocationName": "IslandEast, IslandNorth, IslandSouth, IslandSouthEast, IslandWest" }
		},
		{
			"Name": "SVE Map",
			"Value": "immersive2",
			"When": {
				"HasMod": "flashshifter.immersivefarm2remastered",
				"FarmType": "Standard",
				"HasMod |contains=flashshifter.GrandpasFarm": false
			}
		},
		{
			"Name": "SVE Map",
			"Value": "grandpas",
			"When": {
				"HasMod": "flashshifter.GrandpasFarm",
				"FarmType": "Standard",
				"HasMod |contains=flashshifter.immersivefarm2remastered": false
			}
		},
		{
			"Name": "OliVic",
			"Value": "Vanilla",
			"When": { "HasMod": "grapeponta.VibrantPastoralRecolor" }
		},
		{
			"Name": "OliVic",
			"Value": "Simple Foliage",
			"When": { "HasMod": "Az.Foliage" }
		},
		{
			"Name": "OliVic",
			"Value": "{{Shrubbery}}",
			"When": { "Shrubbery |contains=Simple Foliage, Vanilla": true }
		}
	],
